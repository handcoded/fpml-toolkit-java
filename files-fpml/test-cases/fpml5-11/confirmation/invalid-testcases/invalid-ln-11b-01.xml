    <contractId contractIdScheme="http://www.bankofagentsna.com/messaging/contractids">87125</contractId>
    <facilityReference href="facility" />
    <effectiveDate>2014-04-11</effectiveDate>
    <borrowerPartyReference href="borrower" />
    <amount>
