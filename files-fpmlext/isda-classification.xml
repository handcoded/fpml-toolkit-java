	<item>InterestRate:IRSwap:Basis</item>
	<item>InterestRate:IRSwap:FixedFixed</item>
	<item>InterestRate:IRSwap:FixedFloat</item>
	<item>InterestRate:IRSwap:Inflation</item>
	<item>InterestRate:IRSwap:OIS</item>
	-->
	
	
	<category id="ir" name="{INTEREST-RATE}" superClasses="isda" abstract="true">
		<!-- Any interest rate based product -->
		<xpath test="dyn:swap[not(parent::dyn:swaption)] | dyn:fra | dyn:capFloor | dyn:swaption | dyn:bondOption"/>
	</category>
	
	<category id="ir-exotic-swap" name="InterestRate:Exotic" superClasses="ir" abstract="false">
		<!-- Fallback category for exotic swap products -->
		<xpath test="dyn:swap[not(parent::dyn:swaption)]"/>
	</category>
	
	<category id="ir-swap" name="{INTEREST-RATE:SWAP}" superClasses="ir-exotic-swap" abstract="true">
		<!-- Swaps with two streams in the same currency -->
		<xpath test="count(dyn:swap/dyn:swapStream) = 2  and dyn:swap[dyn:swapStream[1]//dyn:notionalSchedule/dyn:notionalStepSchedule/dyn:currency = dyn:swapStream[2]//dyn:notionalSchedule/dyn:notionalStepSchedule/dyn:currency]"/>
	</category>
	
	<category id="ir-xccy-swap" name="{INTEREST-RATE:SWAP}" superClasses="ir-exotic-swap" abstract="true">
		<!-- Swaps with two streams in the different currencies -->
		<xpath test="count(dyn:swap/dyn:swapStream) = 2  and dyn:swap[dyn:swapStream[1]//dyn:notionalSchedule/dyn:notionalStepSchedule/dyn:currency != dyn:swapStream[2]//dyn:notionalSchedule/dyn:notionalStepSchedule/dyn:currency]"/>
	</category>
	

	
	
	<category id="ir-swap-vanilla" name="{RATES-IR SWAP-VANILLA}" superClasses="ir-swap" abstract="true">
		<!-- No inflation streams and not OIS -->
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:calculationPeriodAmount/dyn:calculation/dyn:inflationRateCalculation) = 0 and count(dyn:swap/dyn:swapStream [//dyn:calculationPeriodFrequency/dyn:period = 'T']) = 0"/>
	</category>
	
	<category name="IR Swap:Fixed-Fixed" superClasses="ir-swap-vanilla">
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:resetDates) = 0"/>
	</category>
	<category name="IR Swap:Fixed-Float" superClasses="ir-swap-vanilla">
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:resetDates) = 1"/>
	</category>
	<category name="IR Swap:Basis" superClasses="ir-swap-vanilla">
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:resetDates) = 2"/>
	</category>
	
	<category name="IR Swap:Inflation" superClasses="ir-swap">
		<!-- Only swaps with at least one inflation stream -->
		<xpath test="dyn:swap[count(dyn:swapStream/dyn:calculationPeriodAmount/dyn:calculation/dyn:inflationRateCalculation) &gt; 0]"/>
	</category>
	
	<category name="IR Swap:OIS" superClasses="ir-swap-vanilla">
		<!-- Only swaps with two single period streams -->
		<xpath test="count(dyn:swap/dyn:swapStream [//dyn:calculationPeriodFrequency/dyn:period = 'T']) = 2"/>
	</category>
	
	<category name="InterestRate:FRA" superClasses="ir">
		<xpath test="dyn:fra"/>
	</category>
	
	<category name="InterestRate:CapFloor" superClasses="ir">
		<xpath test="dyn:capFloor/dyn:capFloorStream"/>
	</category>
	
	<category id="rates-xccy" name="{RATES-XCCY SWAP}" superClasses="ir" abstract="true">
		<!-- Only allow cross-currency swaps with no step schedules and no non 1.0 multipliers -->
		<xpath test="dyn:swap[not(parent::dyn:swaption)] and count(dyn:swap/dyn:swapStream) = 2 and dyn:swap[dyn:swapStream[1]//dyn:notionalSchedule/dyn:notionalStepSchedule/dyn:currency != dyn:swapStream[2]//dyn:notionalSchedule/dyn:notionalStepSchedule/dyn:currency] and count(//dyn:step) = 0 and count(dyn:multiplier[number(text()) != 1]) = 0"/>
	</category>
	<category name="Cross Currency:Fixed-Fixed" superClasses="rates-xccy">
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:resetDates) = 0"/>
	</category>
	<category name="Cross Currency:Fixed-Float" superClasses="rates-xccy">
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:resetDates) = 1"/>
	</category>
	<category name="Cross Currency:Basis" superClasses="rates-xccy">
		<xpath test="count(dyn:swap/dyn:swapStream/dyn:resetDates) = 2"/>
	</category>
	
	<category id="ir-option" name="{RATES-OPTION}" superClasses="ir" abstract="true"/>
	
	<category name="InterestRate:Option:DebtOption" superClasses="ir-option">
		<xpath test="dyn:bondOption"/>
	</category>
	<category name="InterestRate:Option:Swaption" superClasses="ir-option">
		<xpath test="dyn:swaption"/>
	</category>

</classification>
